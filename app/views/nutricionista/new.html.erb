<h1> Smart Diet </h1>
<h2> Cadastro de nutricionista </h2>
 <%= form_with model: @nutricionista do |n| %> <br> <br>
  <%= n.label :nome %> <br> <br>
  <%= n.text_field :nome %> <br> <br>
  <%= n.label :id %> <br> <br>
  <%= n.text_field :id %> <br> <br>
  <%= n.label :email %> <br> <br>
  <%= n.text_field :email %> <br> <br>
  <%= n.label :exp %> <br> <br>
  <%= n.text_field :exp %> <br> <br> <br>
  <%= n.submit "Cadastrar" %>
<% end %>