<p id="notice"><%= notice %></p>

<h1>Consulta</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Sexo</th>
      <th>Idade</th>
      <th>Email</th>
      <th>Profissional</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consulta.each do |consultum| %>
      <tr>
        <td><%= consultum.nome %></td>
        <td><%= consultum.sexo %></td>
        <td><%= consultum.idade %></td>
        <td><%= consultum.email %></td>
        <td><%= consultum.profissional %></td>
        <td><%= consultum.descricao %></td>
        <td><%= link_to 'Mostrar', consultum %></td>
        <td><%#= link_to 'Editar', edit_consultum_path(consultum) %></td>
        <td><%= link_to 'Excluir', consultum, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

